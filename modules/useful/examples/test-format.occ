#INCLUDE "useful.module"

PROC main (SHARED CHAN BYTE out!)
  MOBILE []BYTE s:
  SEQ
    prints.s ("Hello, world!*n", out!)
    SEQ i = 1 FOR 5
      prints.isin (i, " squared is ", i * i, out!)

    CLAIM out!
      print.ssin ("hello, ", "world ", 42, out!)

    format.sin ("life = ", 42, s)
    prints.s (s, out!)
:
