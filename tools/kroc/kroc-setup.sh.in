PATH="@KROC_ARCHBINDIR@:$PATH"
if [ "x$@ENV_LIBRARY_PATH@" = "x" ] ; then
	@ENV_LIBRARY_PATH@="@KROC_ARCHLIBDIR@"
else
	@ENV_LIBRARY_PATH@="@KROC_ARCHLIBDIR@:$@ENV_LIBRARY_PATH@"
fi
export @ENV_LIBRARY_PATH@
PKG_CONFIG_PATH="@KROC_ARCHLIBDIR@/pkgconfig:$PKG_CONFIG_PATH"; export PKG_CONFIG_PATH
MANPATH=":@KROC_MANDIR@:$MANPATH"; export MANPATH
ACLOCAL="${ACLOCAL:-aclocal} -I @ACLOCALDIR@"; export ACLOCAL

