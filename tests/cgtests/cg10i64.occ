-- version control marker $Id: cg10i64.occ,v 1.3 1995/10/09 16:42:33 phw Exp $
PROC do.INT64 ()
  INT64 a, b, c :
  SEQ
    b := id.INT64(85 (INT64))
    --{{{  output message
    title ("INT64 arithmetic")
    --}}} 
    --{{{  check arithmetic and logical operators  (I64-000 to 099)
    a := b + 23 (INT64)
    check.INT64 (a, 85(INT64) + 23(INT64), "I64-001")
    a := b PLUS 23 (INT64)
    check.INT64 (a, 85(INT64) PLUS 23(INT64), "I64-002")
    a := b - 23 (INT64)
    check.INT64 (a, 85(INT64) - 23(INT64), "I64-003")
    a := b MINUS 23 (INT64)
    check.INT64 (a, 85(INT64) MINUS 23(INT64), "I64-004")
    a := b * 23 (INT64)
    check.INT64 (a, 85(INT64) * 23(INT64), "I64-005")
    a := b TIMES 23 (INT64)
    check.INT64 (a, 85(INT64) TIMES 23(INT64), "I64-006")
    a := b / 23 (INT64)
    check.INT64 (a, 85(INT64) / 23(INT64), "I64-007")
    a := b \ 23 (INT64)
    check.INT64 (a, 85(INT64) \ 23(INT64), "I64-008")
    --{{{  check logical operators                 (I64-009 to 014)
    a := b << 3
    check.INT64 (a, 85(INT64) << 3, "I64-009")
    a := b >> 3
    check.INT64 (a, 85(INT64) >> 3, "I64-010")
    a := b /\ 23 (INT64)
    check.INT64 (a, 85(INT64) /\ 23(INT64), "I64-011")
    a := b \/ 23 (INT64)
    check.INT64 (a, 85(INT64) \/ 23(INT64), "I64-012")
    a := b >< 23 (INT64)
    check.INT64 (a, 85(INT64) >< 23(INT64), "I64-013")
    a := ~b
    check.INT64 (a, ~85(INT64), "I64-014")
    --}}} 
    --{{{  unary minus                             (I64-015 to 018)
    a := (- b)
    check.INT64 (a, -85(INT64), "I64-015")
    a := (MINUS b)
    check.INT64 (a, MINUS 85(INT64), "I64-016")
    --{{{  constant folding
    a := MINUS (MOSTNEG INT64)
    check.INT64 (a, MOSTNEG INT64, "I64-017")
    --}}} 
    c := MOSTNEG INT64
    a := MINUS c
    check.INT64 (a, c, "I64-018")
    --}}} 
    --{{{  sign extension                          (I64-019 to 029)
    PROC extension()
      PROC local(VAL INT64 w, x, y, z, INT i)
        i := ((INT w) + (INT x)) + ((INT y) + (INT z))
      :
      INT i :
      SEQ
        local(-1(INT64), -1(INT64), -1(INT64), -1(INT64), i)
        check(i, -4, "I64-019")
        local(1(INT64), 1(INT64), 1(INT64), 1(INT64), i)
        check(i, 4, "I64-020")
    :
    extension()
    --}}}
    --{{{  overflow checks on logical operators    (I64-030 to 039)
    INT64 p, q, r :
    SEQ
      p := id.INT64(MOSTPOS INT64)
      q := p PLUS 256(INT64)
      check.INT64(q, (MOSTNEG INT64) + 255(INT64), "I64-030")
    
      p := id.INT64(MOSTNEG INT64)
      q := p PLUS (-256(INT64))
      check.INT64(q, (MOSTPOS INT64) - 255(INT64), "I64-031")
    
      p := id.INT64(MOSTPOS INT64)
      q := p MINUS (-256(INT64))
      check.INT64(q, (MOSTNEG INT64) + 255(INT64), "I64-032")
    
      p := id.INT64(MOSTNEG INT64)
      q := p MINUS 256(INT64)
      check.INT64(q, (MOSTPOS INT64) - 255(INT64), "I64-033")
    
      p := id.INT64(MOSTPOS INT64)
      q := p TIMES 2(INT64)
      check.INT64(q, (MOSTPOS INT64) TIMES 2(INT64), "I64-034")
      check.INT64(q, (MOSTPOS INT64) << 1,           "I64-035")
    
      p := id.INT64(MOSTPOS INT64)
      q := MINUS p
      check.INT64(q, 0(INT64) MINUS (MOSTPOS INT64), "I64-036")
      check.INT64(q, (MOSTNEG INT64) + 1(INT64),     "I64-037")
    
      r := id.INT64(0(INT64))
      p := id.INT64(MOSTPOS INT64)
      q := (p PLUS 256(INT64)) + r
      check.INT64(q, (MOSTNEG INT64) + 255(INT64), "I64-038")
    
    --}}}
    
    --}}} 
    --{{{  check constant folding                  (I64-100 to 199)
    INT64 a, b :
    INT c :
    SEQ
      b := id.INT64(5 (INT64))
      c := 3
      --{{{  +        (I64-100)
      a := 2 (INT64) + b               -- 7
      a := (1 (INT64) + b    ) + a     -- 6
      a := (0 (INT64) + b    ) + a     -- 5
      a := ((-1 (INT64)) + b ) + a     -- 4
      a := ((-2 (INT64)) + b ) + a     -- 3
      a := (b + 2 (INT64)    ) + a     -- 7
      a := (b + 1 (INT64)    ) + a     -- 6
      a := (b + 0 (INT64)    ) + a     -- 5
      a := (b + (-1 (INT64)) ) + a     -- 4
      a := (b + (-2 (INT64)) ) + a     -- 3
      check.INT64 (a, 50 (INT64), "I64-100")
      --}}} 
      --{{{  -        (I64-101)
      a := 2 (INT64) - b               --  -3
      a := (1 (INT64) - b    ) + a     --  -4
      a := (0 (INT64) - b    ) + a     --  -5
      a := ((-1 (INT64)) - b ) + a     --  -6
      a := ((-2 (INT64)) - b ) + a     --  -7
      a := (b - 2 (INT64)    ) + a     --   3
      a := (b - 1 (INT64)    ) + a     --   4
      a := (b - 0 (INT64)    ) + a     --   5
      a := (b - (-1 (INT64)) ) + a     --   6
      a := (b - (-2 (INT64)) ) + a     --   7
      check.INT64 (a, 0 (INT64), "I64-101")
      --}}} 
      --{{{  *        (I64-102)
      a := 2 (INT64) * b                 --  10
      a := (1 (INT64) * b      ) + a     --   5
      a := (0 (INT64) * b      ) + a     --   0
      a := ((-1 (INT64)) * b   ) + a     --  -5
      a := ((-2 (INT64)) * b   ) + a     -- -10
      a := (b * 2 (INT64)      ) + a     --  10
      a := (b * 1 (INT64)      ) + a     --   5
      a := (b * 0 (INT64)      ) + a     --   0
      a := (b * (-1 (INT64))   ) + a     --  -5
      a := (b * (-2 (INT64))   ) + a     -- -10
      check.INT64 (a, 0 (INT64), "I64-102")
      --}}} 
      --{{{  /        (I64-103)
      a := 2 (INT64) / b                 -- 0
      a := (1 (INT64) / b      ) + a     -- 0
      a := (0 (INT64) / b      ) + a     -- 0
      a := ((-1 (INT64)) / b   ) + a     -- 0
      a := ((-2 (INT64)) / b   ) + a     -- 0
      a := (b / 2 (INT64)      ) + a     -- 2
      a := (b / 1 (INT64)      ) + a     -- 5
      --a := b / 0 (INT64)  --should give error
      a := (b / (-1 (INT64))   ) + a     -- -5
      a := (b / (-2 (INT64))   ) + a     -- -2
      check.INT64 (a, 0 (INT64), "I64-103")
      --}}} 
      --{{{  \        (I64-104)
      a := 2 (INT64) \ b                 --  2
      a := (1 (INT64) \ b      ) + a     --  1
      a := (0 (INT64) \ b      ) + a     --  0
      a := ((-1 (INT64)) \ b   ) + a     -- -1
      a := ((-2 (INT64)) \ b   ) + a     -- -2
      a := (b \ 2 (INT64)      ) + a     --  1
      a := (b \ 1 (INT64)      ) + a     --  0
      --a := b \ 0 (INT64)  --should give error
      a := (b \ (-1 (INT64))   ) + a     --  0
      a := (b \ (-2 (INT64))   ) + a     --  1
      check.INT64 (a, 2 (INT64), "I64-104")
      --}}} 
      --{{{  PLUS     (I64-105)
      a := 2 (INT64) PLUS b                 -- 7
      a := (1 (INT64) PLUS b      ) + a     -- 6
      a := (0 (INT64) PLUS b      ) + a     -- 5
      a := ((-1 (INT64)) PLUS b   ) + a     -- 4
      a := ((-2 (INT64)) PLUS b   ) + a     -- 3
      a := (b PLUS 2 (INT64)      ) + a     -- 7
      a := (b PLUS 1 (INT64)      ) + a     -- 6
      a := (b PLUS 0 (INT64)      ) + a     -- 5
      a := (b PLUS (-1 (INT64))   ) + a     -- 4
      a := (b PLUS (-2 (INT64))   ) + a     -- 3
      check.INT64 (a, 50 (INT64), "I64-105")
      --}}} 
      --{{{  MINUS    (I64-106)
      a := 2 (INT64) MINUS b                  --  -3
      a := (1 (INT64) MINUS b       ) + a     --  -4
      a := (0 (INT64) MINUS b       ) + a     --  -5
      a := ((-1 (INT64)) MINUS b    ) + a     --  -6
      a := ((-2 (INT64)) MINUS b    ) + a     --  -7
      a := (b MINUS 2 (INT64)       ) + a     --   3
      a := (b MINUS 1 (INT64)       ) + a     --   4
      a := (b MINUS 0 (INT64)       ) + a     --   5
      a := (b MINUS (-1 (INT64))    ) + a     --   6
      a := (b MINUS (-2 (INT64))    ) + a     --   7
      check.INT64 (a, 0 (INT64), "I64-106")
      --}}} 
      --{{{  TIMES    (I64-107)
      a := 2 (INT64) TIMES b                --  10
      a := (1 (INT64) TIMES b     ) + a     --   5
      a := (0 (INT64) TIMES b     ) + a     --   0
      a := ((-1 (INT64)) TIMES b  ) + a     --  -5
      a := ((-2 (INT64)) TIMES b  ) + a     -- -10
      a := (b TIMES 2 (INT64)     ) + a     --  10
      a := (b TIMES 1 (INT64)     ) + a     --   5
      a := (b TIMES 0 (INT64)     ) + a     --   0
      a := (b TIMES (-1 (INT64))  ) + a     --  -5
      a := (b TIMES (-2 (INT64))  ) + a     -- -10
      check.INT64 (a, 0 (INT64), "I64-107")
      --}}} 
      --{{{  /\       (I64-108)
      a := 2 (INT64) /\ b                   -- 0
      a := (1 (INT64) /\ b        ) + a     -- 1
      a := (0 (INT64) /\ b        ) + a     -- 0
      a := ((-1 (INT64)) /\ b     ) + a     -- 5
      a := ((-2 (INT64)) /\ b     ) + a     -- 4
      a := (b /\ 2 (INT64)        ) + a     -- 0
      a := (b /\ 1 (INT64)        ) + a     -- 1
      a := (b /\ 0 (INT64)        ) + a     -- 0
      a := (b /\ (-1 (INT64))     ) + a     -- 5
      a := (b /\ (-2 (INT64))     ) + a     -- 4
      check.INT64 (a, 20 (INT64), "I64-108")
      a := b /\ 0(INT64)
      check.INT64 (a, 0(INT64), "I64-108a")
      a := b /\ (-1(INT64))
      check.INT64 (a, 5(INT64), "I64-108b")
      a := b /\ b
      check.INT64 (a, 5(INT64), "I64-108c")
      INT64 bb :
      SEQ
        bb := id.INT64(#00010001FFFF0005(INT64))
        a := bb /\ 0(INT64)
        check.INT64 (a, 0(INT64), "I64-108d")
        a := bb /\ (-1(INT64))
        check.INT64 (a, #00010001FFFF0005(INT64), "I64-108e")
        a := bb /\ #00010001FFFF0005(INT64)
        check.INT64 (a, #00010001FFFF0005(INT64), "I64-108f")
        a := bb /\ #00000000FFFFFFFF(INT64)
        check.INT64 (a, #00000000FFFF0005(INT64), "I64-108g")
        a := bb /\ #FFFFFFFF00000000(INT64)
        check.INT64 (a, #0001000100000000(INT64), "I64-108h")
      
      --}}} 
      --{{{  \/       (I64-109)
      a := 2 (INT64) \/ b                 --  7
      a := (1 (INT64) \/ b      ) + a     --  5
      a := (0 (INT64) \/ b      ) + a     --  5
      a := ((-1 (INT64)) \/ b   ) + a     -- -1
      a := ((-2 (INT64)) \/ b   ) + a     -- -1
      a := (b \/ 2 (INT64)      ) + a     --  7
      a := (b \/ 1 (INT64)      ) + a     --  5
      a := (b \/ 0 (INT64)      ) + a     --  5
      a := (b \/ (-1 (INT64))   ) + a     -- -1
      a := (b \/ (-2 (INT64))   ) + a     -- -1
      check.INT64 (a, 30 (INT64), "I64-109")
      a := b \/ 0(INT64)
      check.INT64 (a, 5(INT64), "I64-109a")
      a := b \/ (-1(INT64))
      check.INT64 (a, -1(INT64), "I64-109b")
      a := b \/ b
      check.INT64 (a, 5(INT64), "I64-109c")
      INT64 bb :
      SEQ
        bb := id.INT64(#00010001FFFF0005(INT64))
        a := bb \/ 0(INT64)
        check.INT64 (a, #00010001FFFF0005(INT64), "I64-109d")
        a := bb \/ (-1(INT64))
        check.INT64 (a, -1(INT64), "I64-109e")
        a := bb \/ #00010001FFFF0005(INT64)
        check.INT64 (a, #00010001FFFF0005(INT64), "I64-109f")
        a := bb \/ #00000000FFFFFFFF(INT64)
        check.INT64 (a, #00010001FFFFFFFF(INT64), "I64-109g")
        a := bb \/ #FFFFFFFF00000000(INT64)
        check.INT64 (a, #FFFFFFFFFFFF0005(INT64), "I64-109h")
      
      --}}} 
      --{{{  ><       (I64-110)
      a := 2 (INT64) >< b                  --  7
      a := (1 (INT64) >< b       ) + a     --  4
      a := (0 (INT64) >< b       ) + a     --  5
      a := ((-1 (INT64)) >< b    ) + a     -- -6
      a := ((-2 (INT64)) >< b    ) + a     -- -5
      a := (b >< 2 (INT64)       ) + a     --  7
      a := (b >< 1 (INT64)       ) + a     --  4
      a := (b >< 0 (INT64)       ) + a     --  5
      a := (b >< (-1 (INT64))    ) + a     -- -6
      a := (b >< (-2 (INT64))    ) + a     -- -5
      check.INT64 (a, 10 (INT64), "I64-110")
      a := b >< 0(INT64)
      check.INT64 (a, 5(INT64), "I64-110a")
      a := b >< (-1(INT64))
      check.INT64 (a, -6(INT64), "I64-110b")
      a := b >< b
      check.INT64 (a, 0(INT64), "I64-110c")
      INT64 bb :
      SEQ
        bb := id.INT64(#00010001FFFF0005(INT64))
        a := bb >< 0(INT64)
        check.INT64 (a, #00010001FFFF0005(INT64), "I64-110d")
        a := bb >< (-1(INT64))
        check.INT64 (a, #FFFEFFFE0000FFFA(INT64), "I64-110e")
        a := bb >< #00010001FFFF0005(INT64)
        check.INT64 (a, 0(INT64), "I64-110f")
        a := bb >< #00000000FFFFFFFF(INT64)
        check.INT64 (a, #000100010000FFFA(INT64), "I64-110g")
        a := bb >< #FFFFFFFF00000000(INT64)
        check.INT64 (a, #FFFEFFFEFFFF0005(INT64), "I64-110h")
      
      --}}} 
      --{{{  <<       (I64-111)
      a := 64 (INT64) << c                 --  512
      a := (32 (INT64) << c      ) + a     --  256
      a := (0 (INT64) << c       ) + a     --    0
      a := ((-64 (INT64)) << c   ) + a     -- -256
      a := ((-32 (INT64)) << c   ) + a     -- -512
      a := (b << 64              ) + a     --    0
      a := (b << 32              ) + a     --   (5 << 32)
      a := (b << 16              ) + a     --   (5 << 16)
      a := (b << 8               ) + a     --   (5 << 8)
      a := (b << 0               ) + a     --    5
      --a := b << (-64) --should give error
      --a := b << (-32) --should give error
      VAL correct.result IS ( 5(INT64)        + (5(INT64) << 32) ) +
                            ((5(INT64) << 16) + (5(INT64) <<  8) ) :
      check.INT64 (a, correct.result, "I64-111")
      --}}} 
      --{{{  >>       (I64-112)
      a := 64 (INT64) >> c                --  8
      a := (32 (INT64) >> c     ) + a     --  4
      a := (0 (INT64) >> c      ) + a     --  0
      a := ((-64 (INT64)) >> c  ) + a     --  (-64 >> 3)
      a := ((-32 (INT64)) >> c  ) + a     --  (-32 >> 3)
      a := (b >> 64             ) + a     --  0
      a := (b >> 32             ) + a     --  0
      a := (b >> 16             ) + a     --  0
      a := (b >> 8              ) + a     --  0
      a := (b >> 0              ) + a     --  5
      --a := b >> (-64) --should give error
      --a := b >> (-32) --should give error
      VAL correct.result IS 17 (INT64) +
                 ( ( (-64(INT64)) >> 3) + ( (-32(INT64)) >> 3) ) :
      check.INT64 (a, correct.result, "I64-112")
      --}}} 
    --}}} 
    --{{{  check comparison operators              (I64-200 to 299)
    a := id.INT64(47(INT64))
    x := a = b
    check.BOOL (x, FALSE, "I64-205")
    x := a > b
    check.BOOL (x, FALSE, "I64-206")
    x := a >= b
    check.BOOL (x, FALSE, "I64-207")
    x := a < b
    check.BOOL (x, TRUE, "I64-208")
    x := a <= b
    check.BOOL (x, TRUE, "I64-209")
    x := a <> b
    check.BOOL (x, TRUE, "I64-210")
    x := a AFTER b
    check.BOOL (x, FALSE, "I64-211")
    
    --}}} 
    --{{{  check conversions                       (I64-300 to 399)
    --{{{  variable declarations for conversion tests
    REAL64 r64 :
    REAL32 r32 :
    INT64 i64 :
    INT32 i32 :
    INT16 i16 :
    BYTE byte :
    BOOL bool :
    --}}} 
    SEQ
      r64 := REAL64 TRUNC b
      check.REAL64 (r64, REAL64 TRUNC 85(INT64), "I64-301")
      r64 := REAL64 TRUNC (b + 1(INT64))
      check.REAL64 (r64, REAL64 TRUNC 86(INT64), "I64-301a")
      r64 := REAL64 TRUNC (b + b)
      check.REAL64 (r64, REAL64 TRUNC 170(INT64), "I64-301b")
      r64 := REAL64 ROUND b
      check.REAL64 (r64, REAL64 ROUND 85(INT64), "I64-302")
      r64 := REAL64 ROUND (b + 1(INT64))
      check.REAL64 (r64, REAL64 ROUND 86(INT64), "I64-302a")
      r64 := REAL64 ROUND (b + b)
      check.REAL64 (r64, REAL64 ROUND 170(INT64), "I64-302b")
      r32 := REAL32 TRUNC b
      check.REAL32 (r32, REAL32 TRUNC 85(INT64), "I64-303")
      r32 := REAL32 TRUNC (b + 1(INT64))
      check.REAL32 (r32, REAL32 TRUNC 86(INT64), "I64-303a")
      r32 := REAL32 TRUNC (b + b)
      check.REAL32 (r32, REAL32 TRUNC 170(INT64), "I64-303b")
      r32 := REAL32 ROUND b
      check.REAL32 (r32, REAL32 ROUND 85(INT64), "I64-304")
      r32 := REAL32 ROUND (b + 1(INT64))
      check.REAL32 (r32, REAL32 ROUND 86(INT64), "I64-304a")
      r32 := REAL32 ROUND (b + b)
      check.REAL32 (r32, REAL32 ROUND 170(INT64), "I64-304b")
      i64 := INT64 b
      check.INT64 (i64, INT64 85(INT64), "I64-305")
      i32 := INT32 b
      check.INT32 (i32, INT32 85(INT64), "I64-306")
      i16 := INT16 b
      check.INT16 (i16, INT16 85(INT64), "I64-307")
      byte := BYTE b
      check.BYTE (byte, BYTE 85(INT64), "I64-308")
      c := 1(INT64)
      bool := BOOL c
      check.BOOL (bool, BOOL 1(INT64), "I64-309")
    
    --}}} 
    --{{{  check 'remainder identity'              (I64-400 to 499)
    a := ( 23(INT64) * ( b / 23(INT64) ) ) + ( b \ 23(INT64) )
    check.INT64 (a,
      ( 23(INT64) * ( 85(INT64) / 23(INT64) ) ) + ( 85(INT64) \ 23(INT64) ),
      "I64-400")
    check.INT64 (a, b, "I64-401")
    --}}} 
    --{{{  hefty calculations                      (I64-500 to 599)
    --{{{  declarations
    INT64 x, y, z, a, b, c, d :
    VAL n1 IS 1(INT64) :
    VAL n2 IS 2(INT64) :
    VAL n3 IS 3(INT64) :
    VAL n4 IS 1(INT64) / 3(INT64) :
    VAL eight.seven IS 2097152(INT64) : -- 8 to the power 7 (= 2 to the 21)
    --}}} 
    SEQ
      --{{{  initialisation
      x := 8(INT64)
      y := x + x
      z := y - x
      a := 111111(INT64)
      b := 222222(INT64)
      c := a * 2(INT64)
      --}}} 
      check.INT64 (y, 16(INT64), "I64-500")
      check.INT64 (z,  8(INT64), "I64-501")
      check.INT64 (c,  b, "I64-502")
      check.INT64 (((x + y) + (n3 * z)) + ((n2 * a) - b), 48(INT64), "I64-503")
      check.INT64 (((x PLUS y) PLUS (n3 TIMES z)) PLUS ((n2 TIMES a) MINUS b), 48(INT64), "I64-503a")
      check.INT64 (x + (x + (x + (x + (x + (x + x))))), 56(INT64), "I64-504")
      check.INT64 (x PLUS (x PLUS (x PLUS (x PLUS (x PLUS (x PLUS x))))), 56(INT64), "I64-504a")
      check.INT64 (x * (x * (x * (x * (x * (x * x))))), eight.seven, "I64-505")
      check.INT64 (x TIMES (x TIMES (x TIMES (x TIMES (x TIMES (x TIMES x))))), eight.seven, "I64-505a")
      check.INT64 (x * (x * (n2 * (y * (y * (x * x))))), eight.seven, "I64-506")
      check.INT64 (x TIMES (x TIMES (n2 TIMES (y TIMES (y TIMES (x TIMES x))))), eight.seven, "I64-506a")
      x := 1 (INT64)
      y := 3 (INT64)
      d := 2 (INT64)
      check.INT64 (x / y, n4, "I64-507")
      c := ((d * y) * (d * y)) * ((d * y) * (d * y))
      check.INT64 (c, 1296 (INT64), "I64-509")
      c := ((d TIMES y) TIMES (d TIMES y)) TIMES ((d TIMES y) TIMES (d TIMES y))
      check.INT64 (c, 1296 (INT64), "I64-509a")
      c := ((((d * y) * (d * y)) * (d * y)) * (d * y)) * (d * y)
      check.INT64 (c, 7776 (INT64), "I64-510")
      c := ((((d TIMES y) TIMES (d TIMES y)) TIMES (d TIMES y)) TIMES (d TIMES y)) TIMES (d TIMES y)
      check.INT64 (c, 7776 (INT64), "I64-510a")
    
    --}}} 
    --{{{  passing as parameters                   (I64-600 to 699)
    --{{{  copy.int64 and add.int64
    PROC copy.int64 (VAL INT64 x, INT64 y)
      y := x
    :
    PROC add.int64 (VAL INT64 x, y, INT64 z)
      z := x + y
    :
    --}}} 
    INT64 x, y, z :
    VAL n1 IS 2(INT64) :
