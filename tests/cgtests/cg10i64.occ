    --{{{  more constant folding (bug INSdi02215, INSdi01895)
    PROC constant.TIMES(VAL INT64 y)
    
      VAL zero.zero IS          0(INT64) :
      VAL zero.one  IS          1(INT64) :
      VAL zero.any  IS          2(INT64) :
      VAL one.zero  IS #100000000(INT64) :
      VAL one.one   IS #100000001(INT64) :
      VAL one.any   IS #100000002(INT64) :
      VAL any.zero  IS #200000000(INT64) :
      VAL any.one   IS #200000001(INT64) :
      VAL any.any   IS #200000002(INT64) :
      VAL table IS [0(INT64), 1(INT64), 2(INT64), -1(INT64), -2(INT64),
                    MOSTNEG INT64, MOSTPOS INT64, 10(INT64),
                    zero.any,
                    one.zero, one.one, one.any,
                    any.zero, any.one, any.any,
                    -zero.any,
                    -one.zero, -one.one, -one.any,
                    -any.zero, -any.one, -any.any ] :
      SEQ
        SEQ z = 0 FOR SIZE table
          PROC local.check(VAL INT64 x, y, VAL []BYTE msg)
            check.INT64(x, y TIMES table[z], msg)
          :
          INT64 res, x :
          SEQ
            x := id.INT64(table[z])
    
            res := x TIMES zero.zero
            local.check(res, zero.zero, "INT64-a")
            res := x TIMES zero.one
            local.check(res, zero.one, "INT64-b")
            res := x TIMES zero.any
            local.check(res, zero.any, "INT64-c")
          
            res := x TIMES one.zero
            local.check(res, one.zero, "INT64-d")
            res := x TIMES one.one
            local.check(res, one.one, "INT64-e")
            res := x TIMES one.any
            local.check(res, one.any, "INT64-f")
          
            res := x TIMES any.zero
            local.check(res, any.zero, "INT64-g")
            res := x TIMES any.one
            local.check(res, any.one, "INT64-h")
            res := x TIMES any.any
            local.check(res, any.any, "INT64-i")
          
            res := zero.zero TIMES x
            local.check(res, zero.zero, "INT64-j")
            res := zero.one  TIMES x
            local.check(res, zero.one, "INT64-k")
            res := zero.any  TIMES x
            local.check(res, zero.any, "INT64-l")
          
            res := one.zero  TIMES x
            local.check(res, one.zero, "INT64-m")
            res := one.one   TIMES x
            local.check(res, one.one, "INT64-n")
            res := one.any   TIMES x
            local.check(res, one.any, "INT64-o")
          
            res := any.zero  TIMES x
            local.check(res, any.zero, "INT64-p")
            res := any.one   TIMES x
            local.check(res, any.one, "INT64-q")
            res := any.any   TIMES x
            local.check(res, any.any, "INT64-r")
    
            res := y         TIMES x
            local.check(res, y, "INT64-s")
    :
    constant.TIMES(27(INT64))
    --}}}
